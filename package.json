{
  "version": "0.2.1",
  "scripts": {
    "postinstall": "jetify",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "dev": "expo start",
    "test": "jest",
    "publish:test": "API_BASE_URL=https://backend-test.actone.live expo publish --target managed",
    "check-types": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.1.0",
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/picker": "1.6.0",
    "@react-navigation/native": "^5.7.2",
    "@react-navigation/stack": "^5.8.0",
    "@types/color-convert": "^1.9.0",
    "@types/react-gateway": "^2.8.3",
    "@welldone-software/why-did-you-render": "^4.0.5",
    "color-convert": "^2.0.1",
    "expo": "^38.0.8",
    "expo-auth-session": "^1.4.1",
    "expo-av": "~8.2.1",
    "expo-constants": "^9.1.1",
    "expo-font": "~8.2.1",
    "expo-google-app-auth": "^8.1.2",
    "expo-speech": "~8.2.1",
    "expo-web-browser": "^8.3.1",
    "google-palette": "1.1.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "0.62.2",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "~1.9.0",
    "react-native-reanimation": "^0.0.6",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-section-list-get-item-layout": "^2.2.3",
    "react-native-svg": "^12.1.0",
    "react-native-unimodules": "~0.10.1",
    "react-native-url-polyfill": "^1.2.0",
    "react-native-web": "~0.11.7",
    "react-query": "^2.5.13",
    "sentry-expo": "^2.1.2",
    "set-cookie-parser": "^2.4.6",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@types/jest": "^24.0.25",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-preset-expo": "^8.2.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^1.3.1",
    "jest-expo": "^38.0.0",
    "jest-styled-components": "^6.3.4",
    "jetifier": "^1.6.4",
    "lint-staged": "^9.5.0",
    "mock-async-storage": "^2.2.0",
    "prettier": "^1.16.4",
    "react-native-console-time-polyfill": "^1.2.1",
    "react-native-testing-library": "^2.1.0",
    "react-test-renderer": "^16.12.0",
    "typescript": "~3.9.5"
  },
  "jest": {
    "projects": [
      {
        "preset": "jest-expo/ios",
        "transformIgnorePatterns": [
          "node_modules/(?!(jest-)?|react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry|@expo-google-fonts/.*)"
        ]
      },
      {
        "preset": "jest-expo/android",
        "transformIgnorePatterns": [
          "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry|@expo-google-fonts/.*)"
        ]
      }
    ],
    "collectCoverageFrom": [
      "App.tsx",
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/contexts/*.ts",
      "!src/**/*.d.ts"
    ]
  },
  "private": true,
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 10,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "prettier/@typescript-eslint",
      "prettier/react",
      "prettier/standard"
    ],
    "plugins": [
      "react",
      "prettier",
      "@typescript-eslint"
    ],
    "rules": {
      "arrow-parens": [
        "warn",
        "as-needed"
      ],
      "strict": [
        2,
        "never"
      ],
      "consistent-return": 0,
      "no-confusing-arrow": 0,
      "no-return-assign": 0,
      "react/destructuring-assignment": [
        1,
        "always",
        {
          "ignoreClassFields": true
        }
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --config ./lint-staged.config.js"
    }
  },
  "lint-staged": {}
}
